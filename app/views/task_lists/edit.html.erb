<h2 class="left-10">Edit a Task List</h2>
<p class="bg-success" id="no_task">
  <% if flash[:notice] %>
    <%= flash[:notice] %><br>
  <% end %>
</p>
<%= form_for @task_list do |f| %>
  <div>
    <%= f.label :name, { :class => 'my_label' }  %><br>
    <%= f.text_field :name, { :class => 'my_textfield' } %>
  </div><br>
  <div>
    <%= f.submit "Update Task List", { :class => 'my_button' }  %>
  </div>

<% end %>
